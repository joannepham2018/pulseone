<?xml version="1.0" encoding="UTF-8"?>
<managedAppConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <version>1</version>
    <bundleId>com.f-secure.freedome</bundleId>
    <dict>
        <string keyName="psbRegistrationKey">
            <constraint max="24"/>
        </string>
        <string keyName="psbApiUrl">
            <constraint pattern="https://([a-zA-Z0-9-]+\.)+[a-zA-Z]+"/>
        </string>
        <string keyName="firstName">
            <defaultValue>
                <userVariable value="firstName"/>
            </defaultValue>
            <constraint nullable="true" max="120"/>
        </string>
        <string keyName="lastName">
            <defaultValue>
                <userVariable value="lastName"/>
            </defaultValue>
            <constraint nullable="true" max="120"/>
        </string>
        <string keyName="email">
            <defaultValue>
                <userVariable value="emailAddress"/>
            </defaultValue>
            <constraint nullable="true" max="200"/>
        </string>
        <string keyName="phone">
            <defaultValue>
                <deviceVariable value="phoneNumber"/>
            </defaultValue>
            <constraint nullable="true" max="20"/>
        </string>
        <string keyName="model">
            <defaultValue>
                <deviceVariable value="model"/>
            </defaultValue>
            <constraint nullable="true" max="100"/>
        </string>
        <string keyName="imei">
            <defaultValue>
                <deviceVariable value="imei"/>
            </defaultValue>
            <constraint nullable="true" max="36"/>
        </string>
    </dict>
